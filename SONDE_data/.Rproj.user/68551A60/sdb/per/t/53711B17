sible pixel (good for looking at density)\n\ncolors() #To find out all colors\npoint.colors <- c(\"red\", \"orange\", \"green\", \"blue\",  \n                  \"magenta\")\nplot(x = primates$Bodywt, y = primates$Brainwt,\n     xlim=c(0,300), ylim=c(0,1400), \n     cex=2, pch=19, col=point.colors) \n#Color chart  - use hexidecimal number\n\n?points\n#Like 21, outide col=blue, inside bg=red \nplot(x = primates$Bodywt, y = primates$Brainwt,\n     xlim=c(0,300), ylim=c(0,1400), \n     cex=2, pch=21, col=\"black\", bg=\"salmon\")\n#Don't use ?plot... most stuff is not under this...\n#Use ?par... most stuff is here...\n#Really don't call par() b/c will change every plot after this\n#Save from this problem - use clear all button\n\npch = c(\"A\",\"C\",\"T\",\"G\")\n#Circle size could be proportional to body weight\n\n#-----------------Legends\n#Allows to create a legend that is not related to data\nlegend(x=\"topright\", #where do you want the legend\n       legend=primates[,1], #text strings to be associated with markers\n       pt.bg=1:5, #background color of points\n       pch=21:25, #vector of symbol types\n       bty=\"n\") #no box type\n\n#tickmark labeling \n#plot(....yaxp = c(0, 1500, 3))\n\n#-----------------Exercise 2\npoint.colors <- 1:5\nplot(x = primates$Bodywt, y = primates$Brainwt,\n     xlim=c(0,300), ylim=c(0,1400), \n     cex=2, pch=19, \n     col=point.colors, \n     xaxs=\"i\", yaxs=\"i\", \n     yaxp = c(0,1500,3)\n     xlab=\"Body weight (g)\", ylab=\"Brain weight (g)\", main=\"blah\")\nlegend(x=\"topright\", #where do you want the legend\n       legend=primates[,1], #text strings to be associated with markers\n       col=point.colors, #background color of points\n       pt.cex=2, #pt.cex will change the size of the points, but CEX will change the overall size of the legend\n       pch=19, #vector of symbol types\n       bty=\"n\") #no box type\n\n?legend\n#note - run each time you change one thing... \n# or else you will never find the inevitable error\n\n-----------#plot the following points or text at a certain part of the axis\n#bottom,left, top, right\n\n#Adding text using locator\nlocator(1)\ntext(x=207,y=306,label=\"Gorilla\")\n\n#labels are primates strings, next to position of points\ntext(x = primates$Bodywt, y = primates$Brainwt,\n     labels = primates[,1], pos=4)\n#Can just change location manually\n\n#Plot all datapoints, click on a few places and identify these points\nidentify(x = primates$Bodywt, y = primates$Brainwt,\n         labels = primates[,1], n = 2) #stops at 2 here...\n\n#----------Points and lines\n?lines #gives values for lty (line type) values\n#lwd = line widths\n#types of plots type = points, lines,... \n\n#plots something over something out\nlines(x=seq(50,200,50), y=c(200,450,500,300),\n      type=\"b\", lwd=3, lty=2)\n\npoints(x=seq(100,250,50), y=seq(100,250,50), \n       cex=3, pch=17)\n\n#------------Exercise 3\n?seq\nxblah<-seq(from=-3,to=3,length.out=500)\nnorm<-exp(-xblah^2)/sqrt(2*pi)\nplot(x=xblah,\n     y=norm,\n     col=\"blue\",\n     type=\"l\", \n     lwd=3,\n     lty=1,\n     ylab=\"density\",\n     xaxt=\"n\",\n     main=\"standard normal density\\nsdkjflk\")\nxblah2<-seq(from=-1.96,to=1.96,length.out=500)\nnorm2<-exp(-xblah2^2)/sqrt(2*pi)\n\n#better within lines x[x>-1,96&x>1.96],y[x>-1.96&x<1.96]\nlines(x=xblah2,\n     y=norm2, \n     type=\"h\", \n     lwd=3,\n     lty=1)\n\n",
